{
  "defaultSeverity": "error",
  "extends": ["tslint:recommended", "tslint-config-airbnb"],
  "jsRules": {
    "no-unused-expression": true
  },
  "rules": {
    "trailing-comma": [
      true,
      {
        "multiline": {
          "objects": "always",
          "arrays": "always",
          "functions": "never",
          "typeLiterals": "ignore"
        },
        "esSpecCompliant": true
      }
    ],
    "max-line-length": [true, 130],
    "prefer-template": false,
    "interface-name": [true, "always"],
    "import-name": false,
    "no-console": false,
    "object-literal-sort-keys": false
  },
  "rulesDirectory": []
}
